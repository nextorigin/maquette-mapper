    "mapping",
    "mvc",
    "mvvm",
    "spine",
    "spinejs",
    "reactive"
  ],
  "author": "doublerebel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nextorigin/maquette-mapper/issues"
  },
  "homepage": "https://github.com/nextorigin/maquette-mapper#readme",
  "dependencies": {
    "maquette": "^2.5.0"
  },
  "devDependencies": {
    "iced-coffee-script": "^108.0.11"
  }
}
